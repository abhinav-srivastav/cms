<div class = "header">
  <h3 > Admin Accounts </h3>
  <div style = "text-align: left">
    <%= link_to "Admin Home", admins_path %>
  </div>
</div>

<% if notice %>
  <p id =  "notice"><%= notice %></p>
<% end %>

<table id = "sortable" name = "Image">
  <tbody class = "content">
  	<% @image.each do |img| %>
    	<tr id = "<%= img.id %>">
    		<td >
    			<%= link_to(image_tag(img.image.url(:small))) %>  
    		</td>
    	</tr>
  	<% end %>
	</tbody>
</table>

