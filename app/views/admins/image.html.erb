<div class = "header">
  <h3 > Admin Accounts </h3>
  <%= render "admin_navigation" %>
</div>

<% if notice %>
  <p id =  "notice"><%= notice %></p>
<% end %>

<table id = "sortable" name = "Image">
  <tbody class = "content">
  	<% @image.each do |img| %>
    	<tr id = "<%= img.id %>">
    		<td >
          <%= img.position %>
    			<%= link_to(image_tag(img.image.url(:small))) %>  
    		</td>
    	</tr>
  	<% end %>
	</tbody>
</table>