<%= form_for @admin do |f| %>
  
  <table>
    <legend> Enter details:</legend>
    <tr>
        <td>
            <%= f.label :username %>
        </td>
        <td>
            <%= f.text_field :username %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :password %>
        </td>
        <td>
            <%= f.password_field :password %>
        </td>
    </tr>
    
  </table>
  
<div >
    <%= f.submit %>
</div>
<% end %>