<!DOCTYPE html>
<html>
<head>
  <title>Cms</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %> 
  <%= csrf_meta_tags %>
  <%= yield :javascript %>
</head>
<body>
<div id="user_nav">
  <% if current_user %>
    Welcome,<%= current_user.username %>.
    <%= link_to "Log out", log_out_path %>
    <br />
    <%= link_to "Product", home_products_path %>
    <%= link_to "Admin", admins_path %>
  <% end %>
  <% if notice %>
		<p id =  "notice"><%= notice %></p>
	<% end %>
</div>
<%= yield :scripts %>
<%= yield %>




</body>
</html>
